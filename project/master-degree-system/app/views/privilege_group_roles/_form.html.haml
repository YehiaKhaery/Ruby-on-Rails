= form_for(@privilege_group_role) do |f|
  - if @privilege_group_role.errors.any?
    #error_explanation
      %h2
        = pluralize(@privilege_group_role.errors.count, "error")
        prohibited this privilege_group_role from being saved:
      %ul
        - @privilege_group_role.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :privilegeGroup
    %br/
    = f.text_field :privilegeGroup
  .field
    = f.label :role
    %br/
    = f.text_field :role
  .field
    = f.label :creator
    %br/
    = f.text_field :creator
  .field
    = f.label :modifier
    %br/
    = f.text_field :modifier
  .actions
    = f.submit
