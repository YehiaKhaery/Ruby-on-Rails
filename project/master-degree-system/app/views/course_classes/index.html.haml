%h1 Listing course_classes
%table
  %thead
    %tr
      %th Courseclassname
      %th Creator
      %th Modifier
      %th
      %th
      %th
  %tbody
    - @course_classes.each do |course_class|
      %tr
        %td= course_class.courseClassName
        %td= course_class.creator
        %td= course_class.modifier
        %td= link_to 'Show', course_class
        %td= link_to 'Edit', edit_course_class_path(course_class)
        %td= link_to 'Destroy', course_class, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Course class', new_course_class_path
