= form_for(@student_sci_degree_grade) do |f|
  - if @student_sci_degree_grade.errors.any?
    #error_explanation
      %h2
        = pluralize(@student_sci_degree_grade.errors.count, "error")
        prohibited this student_sci_degree_grade from being saved:
      %ul
        - @student_sci_degree_grade.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :studentSciDegree
    %br/
    = f.text_field :studentSciDegree
  .field
    = f.label :year
    %br/
    = f.number_field :year
  .field
    = f.label :grade
    %br/
    = f.text_field :grade
  .field
    = f.label :creator
    %br/
    = f.text_field :creator
  .field
    = f.label :modifier
    %br/
    = f.text_field :modifier
  .actions
    = f.submit
