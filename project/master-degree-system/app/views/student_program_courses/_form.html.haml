= form_for(@student_program_course) do |f|
  - if @student_program_course.errors.any?
    #error_explanation
      %h2
        = pluralize(@student_program_course.errors.count, "error")
        prohibited this student_program_course from being saved:
      %ul
        - @student_program_course.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :programClassSemesterCourse
    %br/
    = f.text_field :programClassSemesterCourse
  .field
    = f.label :studentProgram
    %br/
    = f.text_field :studentProgram
  .field
    = f.label :gradeClass
    %br/
    = f.text_field :gradeClass
  .field
    = f.label :subscriptionDate
    %br/
    = f.date_select :subscriptionDate
  .field
    = f.label :gradeValue
    %br/
    = f.text_field :gradeValue
  .field
    = f.label :isComplementaryCourse
    %br/
    = f.check_box :isComplementaryCourse
  .field
    = f.label :creator
    %br/
    = f.text_field :creator
  .field
    = f.label :modifier
    %br/
    = f.text_field :modifier
  .actions
    = f.submit
