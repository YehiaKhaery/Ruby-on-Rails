= form_for(@professor_program_class_course) do |f|
  - if @professor_program_class_course.errors.any?
    #error_explanation
      %h2
        = pluralize(@professor_program_class_course.errors.count, "error")
        prohibited this professor_program_class_course from being saved:
      %ul
        - @professor_program_class_course.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :programClassSemesterCourse
    %br/
    = f.select :programClassSemesterCourse, ProgramClassSemesterCourse.all.map{|s| [s.specificCourseName,s.id]} 
  .field.hidden
    = f.label :creator
    %br/
    = f.text_field :creator
  .field.hidden
    = f.label :modifier
    %br/
    = f.text_field :modifier
  .actions
    = f.submit
